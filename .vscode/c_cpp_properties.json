{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${env:HOME}/.arduino15/packages/arduino/hardware/avr/1.8.3/cores/**",
                "${env:HOME}/.arduino15/packages/arduino/hardware/avr/1.8.3/variants/standard/**",
                "${env:HOME}/.arduino15/packages/arduino/hardware/avr/1.8.3/libraries/EEPROM/**",
                "${env:HOME}/.arduino15/packages/arduino/hardware/avr/1.8.3/libraries/HID/**",
                "${env:HOME}/.arduino15/packages/arduino/hardware/avr/1.8.3/libraries/SoftwareSerial/**",
                "${env:HOME}/.arduino15/packages/arduino/hardware/avr/1.8.3/libraries/SPI/**",
                "${env:HOME}/.arduino15/packages/arduino/hardware/avr/1.8.3/libraries/Wire/**",
                "${env:HOME}/Arduino/libraries/Adafruit_NeoPixel"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "F_CPU=16000000L",
                "ARDUINO=10607",
                "ARDUINO_AVR_UNO",
                "ARDUINO_ARCH_AVR"
            ],
            "compilerPath": "${env:HOME}/.arduino15//packages/arduino/tools/avr-gcc/7.3.0-atmel3.6.1-arduino7/bin/avr-gcc",
            "cStandard": "c11",
            "cppStandard": "c++11",
            "intelliSenseMode": "linux-gcc-x64"
        }
    ],
    "version": 4
}